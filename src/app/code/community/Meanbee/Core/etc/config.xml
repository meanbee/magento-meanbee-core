<?xml version="1.0"?>
<config>
    <modules>
        <Meanbee_Core>
            <version>1.0.0</version>
        </Meanbee_Core>
    </modules>
    <global>
        <models>
            <meanbee_core>
                <class>Meanbee_Core_Model</class>
                <resourceModel>meanbee_core_resource</resourceModel>
            </meanbee_core>
            <meanbee_core_resource>
                <class>Meanbee_Core_Model_Resource</class>
            </meanbee_core_resource>
        </models>
        <resources>
            <meanbee_core_setup>
                <setup>
                    <module>Meanbee_Core</module>
                    <class>Meanbee_Core_Model_Resource_Setup</class>
                </setup>
            </meanbee_core_setup>
        </resources>
        <helpers>
            <meanbee_core>
                <class>Meanbee_Core_Helper</class>
            </meanbee_core>
        </helpers>
    </global>
    <phpunit>
        <suite>
            <modules>
                <Meanbee_Core/>
            </modules>
        </suite>
    </phpunit>
</config>

